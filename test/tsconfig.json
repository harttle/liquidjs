{
  "compilerOptions": {
    "target": "es6",
    "module":"CommonJS",
    "lib": ["es2015", "es2016", "es2017", "dom"],
    "sourceMap": true,
    "outDir": "dist",
    "declaration": true,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "downlevelIteration": true,
    "strict": true,
    "suppressImplicitAnyIndexErrors": true
  },
  "all": true
}
